<div class="translate-list-field-head">
    {% for locale in locales %}
        {% set locale = locale|split('_')[0] %}

        {% if locale == 'en' %}
            <img class="translate-list-field-head__flag"
                 src="{{ asset('bundles/lin3scmsadminbridge/images/GB.png') }}"/>
        {% elseif locale == 'es' %}
            <img class="translate-list-field-head__flag"
                 src="{{ asset('bundles/lin3scmsadminbridge/images/ES.png') }}"/>
        {% elseif locale == 'fr' %}
            <img class="translate-list-field-head__flag"
                 src="{{ asset('bundles/lin3scmsadminbridge/images/FR.png') }}"/>
        {% elseif locale == 'eu' %}
            <img class="translate-list-field-head__flag"
                 src="{{ asset('bundles/lin3scmsadminbridge/images/_basque-country.png') }}"/>
        {% endif %}
    {% endfor %}
</div>
